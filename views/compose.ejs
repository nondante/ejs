<%- include("partials/header") -%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="title">Title</label>
    <input class="form-control" id="title" type="text" name="titleValue" value="">
  </div>
  <div class="form-group">
    <label for="post">Post</label>
    <textarea  id="post" class="form-control" rows="5" cols="30" type="text" name="postValue" value=""></textarea>
  </div>
  <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>
<%- include("partials/footer") %>
